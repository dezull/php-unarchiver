{
    "name": "dezull/unarchiver",
    "description": "Extract archives using libarchive tools and unrar",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dezull\\Unarchiver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Dzul Nizam Bin Mahmud Pauzi",
            "email": "dezull@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3.16",
        "symfony/process": "^6.1|^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.16",
        "phpunit/phpunit": "^12",
        "vimeo/psalm": "dev-master",
        "phpstan/phpstan": "^2.1",
        "spatie/temporary-directory": "^2.3"
    },
    "scripts": {
      "test": "TZ=utc vendor/bin/phpunit",
      "coverage": "php -dextension=pcov -dpcov.enabled=1 vendor/bin/phpunit --coverage-text"
    }
}
